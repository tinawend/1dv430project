
{{#if sessuser}}
<h1>Välkommen {{sessuser.username}}</h1>
<p>lägg till information om dig!</p>
{{#if usersprofile}}
{{#each usersprofile}}
<p>{{name}}</p>
<p>{{age}}</p>
    <form id="editbutton" action="/profile/aboutMe/{{id}}" method="GET">
    <input type="hidden" name="_csrf" value="{{csrfToken}}">
        <button type="submit">Ändra</button>
    </form>

    {{/each}}
{{else}}
<form id="addprofile" action="/profile" method="POST">
<input id='name' name='name' placeholder='name'>
<br>
<input id='age' name='age'>
<br>
<input type="hidden" name="_csrf" value="{{csrfToken}}">
<button type="submit">Send</button>
</form>
{{/if}}

   {{/if}}
